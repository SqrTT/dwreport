{
  "name": "dwreporter",
  "description": "Generates report based on dwhint",
  "version": "0.0.1",
  "dependencies": {
    "express": "*",
    "lodash": "*",
    "serve-index": "*",
    "serve-static": "*",
    "cookie-session": "*",
    "dwhint": "git://github.com/SqrTT/dwhint#dw2.4.4",
    "shelljs": "*",
    "body-parser": "*",
    "nedb": "0.11",
    "sloc": "0.1",
    "minimist": "1.1"
  },
  "devDependencies": {
    "node-inspector": "*",
    "nodemon": "*",
    "gulp-bower": "~0.0.7",
    "gulp": "~3.8.10",
    "bower-files": "~2.2.1",
    "gulp-concat": "~2.4.2",
    "gulp-uglify": "~1.0.2",
    "gulp-sourcemaps": "~1.2.8",
    "smodular": "git://github.com/SqrTT/smodular"
  },
  "main": "dwreport",
  "jshintConfig": {
    "maxerr": 1000,
    "nonbsp": true,
    "freeze": true,
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "forin": false,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "undef": true,
    "unused": true,
    "trailing": true,
    "maxlen": 120,
    "maxcomplexity": 12,
    "onevar": true,
    "white": true,
    "quotmark": "single",
    "node": true
  },
  "scripts": {
    "test": "make test",
    "start": "make run"
  }
}
