{
	"name": "dwreporter",
	"description": "Generates report based on dwhint",
	"version": "0.0.1",
	"dependencies": {
		"express" : "*",
		"serve-index" : "*",
		"serve-static" : "*",
		"dwhint" : "git://github.com/SqrTT/dwhint#dw2.4.4",
		"filewalker" : "*",
		"shelljs" : "*"
	},
	"devDependencies": {
	},
	"main" : "dwreport",
	"engines": {
		"node": ">=0.10.1"
	},
	"jshintConfig": {
		"maxerr": 1000,
		"nonbsp": true,
		"freeze": true,
		"bitwise": true,
		"camelcase": true,
		"curly": true,
		"eqeqeq": true,
		"forin": true,
		"indent": 4,
		"latedef": true,
		"newcap": true,
		"noarg": true,
		"noempty": true,
		"nonew": true,
		"undef": true,
		"unused": true,
		"trailing": true,
		"maxlen": 120,
		"maxcomplexity": 15,
		"onevar": false,
		"white": true,
		"quotmark": "double",
		"node": true
	},
	"scripts" : {
		"test" : "make test",
		"start" : "make run"
	}
}
